<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="wrap_content"
            android:layout_height="320dp"
            android:layout_marginTop="30dp"
            android:layout_marginRight="190dp"
            android:src="@drawable/parking_change_1" />


        <ImageView
            android:layout_width="75dp"
            android:layout_height="130dp"
            android:layout_gravity="right"
            android:layout_marginTop="300dp"
            app:srcCompat="@drawable/round8" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="300dp"
            android:layout_height="170dp"
            android:layout_gravity="center"
            android:layout_marginTop="220dp"
            app:srcCompat="@drawable/parking_change_3" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="280dp"
            android:layout_height="170dp"
            android:layout_gravity="center"
            android:layout_marginTop="220dp"
            app:srcCompat="@drawable/parking_change_2" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="270dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="280dp"
            app:srcCompat="@drawable/kickboard_image" />


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="130dp"
                android:layout_marginLeft="40dp"
                android:layout_marginBottom="15dp"
                android:text="주차장에 도착했습니다"
                android:fontFamily="@font/nanum_square_eb"
                android:textSize="30dp"
                android:textColor="@color/black"/>

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginBottom="10dp"
                android:text="주차장 선 안에"
                android:fontFamily="@font/nanum_square_eb"
                android:textSize="30dp"
                android:textColor="@color/black"/>

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginBottom="30dp"
                android:text="정차해주세요"
                android:fontFamily="@font/nanum_square_eb"
                android:textSize="30dp"
                android:textColor="@color/black"/>
        </LinearLayout>
    </FrameLayout>
</LinearLayout>